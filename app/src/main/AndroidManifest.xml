<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.campingapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CampingApp">
        <activity android:name=".reservation.ManageReservationActivity"></activity>
        <activity android:name=".upload.UploadedCampActivity" />
        <activity android:name=".login.SignUpActivity" />
        <activity android:name=".login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".reservation.ReservationActivity" />
        <activity android:name=".upload.UploadInformation" />
        <activity android:name=".reservation.ConfirmReservationActivity" />
        <activity android:name=".reservation.CampInformationActivity" />
        <activity android:name=".review.WritingReview" />
        <activity android:name=".home.MainActivity" />
    </application>

</manifest>